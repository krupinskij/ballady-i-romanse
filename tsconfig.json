{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@db": ["db"],
      "@layouts/*": ["src/layouts/*"],
      "@model": ["src/model"],
      "@schema": ["src/schema"],
      "@styles/*": ["src/styles/*"]
    },
    "types": ["@cloudflare/workers-types/2023-07-01"]
  }
}
