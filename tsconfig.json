{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@db": ["src/db"],
      "@i18n": ["src/i18n"],
      "@layouts/*": ["src/layouts/*"],
      "@model": ["src/model"],
      "@styles/*": ["src/styles/*"],
      "@utils": ["src/utils"]
    },
    "types": ["@cloudflare/workers-types/2023-07-01"]
  }
}
